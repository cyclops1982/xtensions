{
  "myrequestschema": {
    "$schema": "http://json-schema.org/draft-04/schema#",

    "type": "object",
    "required": ["FirstName", "LastName"],
    "properties": {
      "FirstName": {
        "type": "string"
      },
      "LastName": {
        "type": "string"
      },
      "Address": {
        "required": [ "Zipcode" ],
        "type": "object",
        "properties": {
          "Street": {
            "type": "string"
          },
          "Number": {
            "type": "number"
          },
          "Zipcode": {
            "type": "string"
          },
          "State": {
            "type": "string"
          },
          "Country": {
            "type": "string"
          }
        }
      }
    }
  }
}
